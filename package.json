{
  "name": "studiobot-ai",
  "version": "1.0.0",
  "description": "AI-powered video analysis and multi-platform content distribution platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:ui": "npm --prefix ui/web run dev",
    "dev:desktop": "npm --prefix ui/desktop run dev:all",
    "dev:all": "concurrently \"npm run dev:ui\" \"npm run dev:desktop\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest --passWithNoTests"
  },
  "keywords": [
    "video",
    "ai",
    "clips",
    "shorts",
    "multi-platform",
    "youtube",
    "twitch",
    "rumble"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "assemblyai": "^4.23.0",
    "axios": "^1.3.2",
    "cors": "^2.8.5",
    "deps": "^1.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "once": "^1.4.0",
    "root": "^3.2.0",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/jest": "^29.2.4",
    "@types/multer": "^1.4.13",
    "@types/node": "^18.19.130",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.31.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
